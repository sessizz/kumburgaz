@model Kumburgaz.Web.Models.BillingGroupFormViewModel
@{
    ViewData["Title"] = "Aidatlandırma Grubu Düzenle";
}

<h2>Aidatlandırma Grubu Düzenle</h2>

<form asp-action="Edit" method="post" class="mt-3">
    <input asp-for="Id" type="hidden" />
    <partial name="_Form" model="Model" />
    <button class="btn btn-primary" type="submit">Güncelle</button>
    <a class="btn btn-outline-secondary" asp-action="Index">Vazgeç</a>
</form>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}

